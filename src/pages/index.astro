                                                            size: 12,
                                                            weight: '600'
                                                        },
                                                        bodyFont: {
                                                            size: 11,
                                                            weight: '500'
                                                        },
                                                        callbacks: {
                                                            title: function(context) {
                                                                return context[0].label;
                                                            },
                                                            label: function(context) {
                                                                const growth = ((context.parsed.y - 100)).toFixed(0);
                                                                const label = context.dataset.label === 'Traditional SEO' ? 'Traditional' : 'Awkward Media';
                                                                return label + ': +' + growth + '%';
                                                            }
                                                        }
                                                    }
                                                },
                                                scales: {
                                                    x: {
                                                        display: true,
                                                        grid: {
                                                            display: true,
                                                            drawOnChartArea: false,
                                                            drawTicks: true,
                                                            color: '#374151',
                                                            lineWidth: 1,
                                                            tickLength: 8
                                                        },
                                                        ticks: {
                                                            color: '#9CA3AF',
                                                            font: {
