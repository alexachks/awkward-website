    <!-- Timeline Rocket Animation Script -->
    <script>
        document.addEventListener('DOMContentLoaded', function() {
            const timelineContainer = document.getElementById('timeline-container');
            const timelineRocket = document.getElementById('timeline-rocket');
            const timelinePoints = document.querySelectorAll('.timeline-point');
            const rocketTrail = document.getElementById('rocket-trail');
            
            let trailPoints = [];
            let lastProgress = 0;
            let lastScrollDirection = 1; // 1 for down, -1 for up
            let currentAngle = 90; // Start pointing down
            
            if (!timelineContainer || !timelineRocket || !rocketTrail) return;
            
            function updateRocketPosition() {
                const containerRect = timelineContainer.getBoundingClientRect();
                const containerTop = containerRect.top + window.scrollY;
                const containerHeight = containerRect.height;
                const windowHeight = window.innerHeight;
                const scrollY = window.scrollY;
                
                // Calculate how much of the timeline is visible/scrolled
                const timelineStart = containerTop - windowHeight * 0.8;
                const timelineEnd = containerTop + containerHeight - windowHeight * 0.2;
                
                if (scrollY >= timelineStart && scrollY <= timelineEnd) {
                    // Calculate progress through timeline (0 to 1)
                    const progress = (scrollY - timelineStart) / (timelineEnd - timelineStart);
                    const clampedProgress = Math.max(0, Math.min(1, progress));
                    
                    // Move rocket along timeline with serpentine path
                    const rocketPosition = clampedProgress * (containerHeight - 32); // 32px is rocket height
                    
                    // Serpentine horizontal movement (sine wave)
                    const amplitude = 80; // How far left/right the rocket moves
                    const frequency = 3; // How many curves in the path
                    const horizontalOffset = Math.sin(clampedProgress * Math.PI * frequency) * amplitude;
                    
                    // Calculate rotation based on movement direction with smoothing
                    const step = 0.001; // Smaller step for smoother calculation
                    const currentHorizontal = Math.sin(clampedProgress * Math.PI * frequency) * amplitude;
                    const nextHorizontal = Math.sin((clampedProgress + step) * Math.PI * frequency) * amplitude;
                    
                    const horizontalVelocity = nextHorizontal - currentHorizontal;
                    const verticalVelocity = step * containerHeight; // Always positive (moving down)
                    
                    // –ü—Ä–æ—Å—Ç–∞—è –ª–æ–≥–∏–∫–∞ –ø–æ–≤–æ—Ä–æ—Ç–∞ —Ä–∞–∫–µ—Ç—ã –≤ –∑–∞–≤–∏—Å–∏–º–æ—Å—Ç–∏ –æ—Ç –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏—è –¥–≤–∏–∂–µ–Ω–∏—è
                    let targetAngle = 190; // –ë–∞–∑–æ–≤—ã–π —É–≥–æ–ª - –ø–æ–≤–æ—Ä–∞—á–∏–≤–∞–µ–º –Ω–∞ 100 –≥—Ä–∞–¥—É—Å–æ–≤ (90 + 100)
                    
                    if (horizontalVelocity < -0.001) { // –õ–µ—Ç–∏—Ç –≤–ª–µ–≤–æ
                        targetAngle = 170; // –ü–æ–≤–æ—Ä–æ—Ç –≤–ª–µ–≤–æ-–≤–Ω–∏–∑ (70 + 100)
                    } else if (horizontalVelocity > 0.001) { // –õ–µ—Ç–∏—Ç –≤–ø—Ä–∞–≤–æ  
                        targetAngle = 105; // –ï—â–µ –Ω–∞ 5 –≥—Ä–∞–¥—É—Å–æ–≤ —Å–ª–∞–±–µ–µ (110 - 5)
                    }
                    
                    // –î–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω—ã–µ –∫–æ—Ä—Ä–µ–∫—Ç–∏—Ä–æ–≤–∫–∏ –¥–ª—è –ø—Ä–æ–±–ª–µ–º–Ω—ã—Ö –∑–æ–Ω
                    if (clampedProgress >= 0.25 && clampedProgress <= 0.55) {
                        if (horizontalVelocity < 0) {
                            targetAngle = 160; // –õ–µ–≤–µ–µ –≤ –ø—Ä–æ–±–ª–µ–º–Ω–æ–π –∑–æ–Ω–µ (60 + 100)
                        }
                    }
                    
                    // –°–ø–µ—Ü–∏–∞–ª—å–Ω–∞—è –∫–æ—Ä—Ä–µ–∫—Ü–∏—è –¥–ª—è —ç—Ç–∞–ø–∞ "1 MONTH Day 30" (80-90% –ø—Ä–æ–≥—Ä–µ—Å—Å–∞)
                    if (clampedProgress >= 0.8 && clampedProgress <= 0.95) {
                        if (horizontalVelocity > 0.001) { // –õ–µ—Ç–∏—Ç –≤–ø—Ä–∞–≤–æ
                            targetAngle = 120; // –ë–æ–ª–µ–µ —É–º–µ—Ä–µ–Ω–Ω—ã–π –ø–æ–≤–æ—Ä–æ—Ç –≤–ø—Ä–∞–≤–æ (–≤–º–µ—Å—Ç–æ 210¬∞)
                        }
                    }
                    
                    // Smooth angle interpolation for fluid rotation
                    const angleDiff = targetAngle - currentAngle;
                    const maxAngleChange = 3; // Maximum degrees change per frame
                    
                    if (Math.abs(angleDiff) > maxAngleChange) {
                        currentAngle += Math.sign(angleDiff) * maxAngleChange;
                    } else {
                        currentAngle = targetAngle;
                    }
                    
                    const adjustedAngle = currentAngle;
                    
                    // Position rocket at current location
                    timelineRocket.style.top = rocketPosition + 'px';
                    timelineRocket.style.left = '50%';
                    timelineRocket.style.transform = `translateX(calc(-50% + ${horizontalOffset}px)) translateY(-50%)`;
                    timelineRocket.style.opacity = '1';
                    
                    // Update rocket rotation
                    const rocketEmoji = timelineRocket.querySelector('div');
                    rocketEmoji.style.transform = `rotate(${adjustedAngle}deg)`;
                    
                    // Detect scroll direction
                    const currentScrollDirection = clampedProgress >= lastProgress ? 1 : -1;
                    
                    // Check if direction changed from up to down
                    if (lastScrollDirection === -1 && currentScrollDirection === 1) {
                        // Clear trail and start fresh - trail starts FROM the rocket position
                        trailPoints = [];
                    }
                    
                    // Only add trail when moving forward (down), not when scrolling back up
                    if (currentScrollDirection === 1) {
                        // Add trail point - rocket leaves trail BEHIND it
                        // So we add the point where rocket WAS (previous position)
                        if (trailPoints.length === 0) {
                            // First point - start from rocket
                            trailPoints.unshift({
                                x: 100 + horizontalOffset, // Center at 100px (half of 200px width)
                                y: rocketPosition // Current rocket position
                            });
                        } else {
                            // Add trail from the front (rocket leaves trail behind)
                            trailPoints.unshift({
                                x: 100 + horizontalOffset,
                                y: rocketPosition
                            });
                        }
                        
                        // Limit trail length for fading effect
                        if (trailPoints.length > 40) { // Shorter trail for better performance
                            trailPoints.pop(); // Remove from the end (oldest trail)
                        }
                        
                        // Update trail path
                        updateRocketTrail();
                    }
                    
                    // Update direction tracking
                    lastScrollDirection = currentScrollDirection;
                    
                    lastProgress = clampedProgress;
                    
                    // Activate timeline points based on progress
                    const currentStep = Math.floor(clampedProgress * 5); // 5 steps total
                    timelinePoints.forEach((point, index) => {
                        if (index <= currentStep) {
                            point.classList.add('active');
                        } else {
                            point.classList.remove('active');
                        }
                    });
                    
                } else if (scrollY < timelineStart) {
                    // Before timeline starts
                    timelineRocket.style.top = '0px';
                    timelineRocket.style.left = '50%';
                    timelineRocket.style.transform = 'translateX(-50%) translateY(-50%)';
                    timelineRocket.style.opacity = '0.5';
                    const rocketEmoji = timelineRocket.querySelector('div');
                    rocketEmoji.style.transform = 'rotate(90deg)'; // Point straight down at start
                    // Clear trail
                    trailPoints = [];
                    rocketTrail.setAttribute('d', '');
                    lastProgress = 0;
                    lastScrollDirection = 1;
                    currentAngle = 90; // Reset angle to pointing down
                    // Deactivate all points
                    timelinePoints.forEach(point => point.classList.remove('active'));
                } else {
                    // After timeline ends
                    timelineRocket.style.top = (containerHeight - 32) + 'px';
                    timelineRocket.style.left = '50%';
                    timelineRocket.style.transform = 'translateX(-50%) translateY(-50%)';
                    timelineRocket.style.opacity = '0.5';
                    const rocketEmoji = timelineRocket.querySelector('div');
                    rocketEmoji.style.transform = 'rotate(90deg)'; // Point straight down at end
                    // Activate all points
                    timelinePoints.forEach(point => point.classList.add('active'));
                }
            }
            
            function updateRocketTrail() {
                if (trailPoints.length < 2) {
                    rocketTrail.setAttribute('d', '');
                    return;
                }
                
                let pathData = `M ${trailPoints[0].x} ${trailPoints[0].y}`;
                
                for (let i = 1; i < trailPoints.length; i++) {
                    if (i === 1) {
                        pathData += ` L ${trailPoints[i].x} ${trailPoints[i].y}`;
                    } else {
                        // Use quadratic curves for smoother trail
                        const prevPoint = trailPoints[i - 1];
                        const currentPoint = trailPoints[i];
                        const controlX = (prevPoint.x + currentPoint.x) / 2;
                        const controlY = (prevPoint.y + currentPoint.y) / 2;
                        pathData += ` Q ${controlX} ${controlY} ${currentPoint.x} ${currentPoint.y}`;
                    }
                }
                
                rocketTrail.setAttribute('d', pathData);
                
                // Add fading effect - make the trail gradually transparent from tail to head
                const totalLength = trailPoints.length;
                const fadeStart = Math.max(0, totalLength - 30); // Start fading from the last 30 points
                
                // Update stroke-opacity for fading effect
                const baseOpacity = 0.7;
                const fadeOpacity = Math.min(baseOpacity, totalLength / 50); // Fade based on trail length
                rocketTrail.style.strokeOpacity = fadeOpacity;
            }
            
            // Update rocket position on scroll
            window.addEventListener('scroll', updateRocketPosition);
            // Initial positioning
            updateRocketPosition();
        });
    </script>

