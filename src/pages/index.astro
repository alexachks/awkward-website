---
import Layout from '../layouts/Layout.astro';
---

<Layout>
    <style>
        html {
            overscroll-behavior-y: none;
            scroll-behavior: auto;
        }
        
        body {
            overscroll-behavior-y: none;
            background: 
                linear-gradient(rgba(255, 255, 255, 0.05) 1px, transparent 1px),
                linear-gradient(90deg, rgba(255, 255, 255, 0.05) 1px, transparent 1px),
                radial-gradient(circle at 160px 240px, rgba(255, 255, 255, 0.15) 2px, transparent 2px),
                radial-gradient(circle at 480px 160px, rgba(255, 255, 255, 0.12) 2px, transparent 2px),
                radial-gradient(circle at 320px 400px, rgba(255, 255, 255, 0.18) 2px, transparent 2px),
                radial-gradient(ellipse at top, #0a0a0a 0%, #000000 30%, #050505 70%, #000000 100%);
            background-size: 
                120px 120px,
                120px 120px,
                120px 120px,
                120px 120px,
                120px 120px,
                100% 100%;
            transition: background 1s ease-in-out;
        }
        
        
        html, body {
            scroll-snap-type: none;
            -webkit-overflow-scrolling: auto;
            overscroll-behavior: none;
        }
        
        * {
            scroll-behavior: auto;
        }
    </style>
    <!-- Header with Expandable Menu -->
    <div class="fixed top-0 left-0 right-0 z-[200] transition-all duration-300 ease-in-out" id="main-header">
        <!-- Expandable Header -->
        <header class="header-container bg-neutral-950/80 backdrop-blur-lg border-b border-white/10 transition-all duration-300">
        <nav class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 xl:max-w-none xl:px-16">
            <div class="flex justify-between items-center h-16 xl:justify-start xl:space-x-16">
                <div class="flex items-center">
                    <a href="/" class="flex items-center space-x-2 text-white hover:text-gray-300 transition-colors">
                        <!-- cms:text key="header.logo.text" label="Logo Text" -->
                        <span class="text-2xl font-black lowercase tracking-wide">awkward media</span>
                        <!-- /cms:text -->
                    </a>
                </div>
                
                <!-- Navigation Menu Container -->
                <div class="hidden lg:flex items-center space-x-8 xl:flex-1 xl:justify-center xl:space-x-12 relative">
                    <!-- Services Menu Trigger -->
                    <div class="services-trigger cursor-pointer relative">
