                                                    borderColor: '#A855F7',
                                                    backgroundColor: gradient,
                                                    borderWidth: 3,
                                                    pointBackgroundColor: '#A855F7',
                                                    pointBorderColor: '#C084FC',
                                                    pointRadius: 5,
                                                    pointHoverRadius: 8,
                                                    tension: 0.4,
                                                    fill: true
                                                }]
                                            },
                                            options: {
                                                responsive: true,
                                                maintainAspectRatio: false,
                                                interaction: {
                                                    intersect: false,
                                                    mode: 'index'
                                                },
                                                layout: {
                                                    padding: {
                                                        top: 0
                                                    }
                                                },
                                                plugins: {
                                                    legend: {
                                                        display: false
                                                    },
                                                    tooltip: {
                                                        backgroundColor: 'rgba(23, 23, 23, 0.95)',
                                                        titleColor: '#FFFFFF',
                                                        bodyColor: '#D1D5DB',
                                                        borderColor: 'rgba(168, 85, 247, 0.3)',
