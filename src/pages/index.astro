                                                        hoverBackgroundColor: '#FFFFFF',
                                                        hoverBorderWidth: 3
                                                    }
                                                },
                                                animation: {
                                                    duration: 2500,
                                                    easing: 'easeOutQuart'
                                                }
                                            }
                                        });
                                        }); // End of loadChartJS().then()
                                    }
                                    
                                    // Initialize with Intersection Observer for better performance
                                    function observeChart() {
                                        const chartContainer = document.getElementById('trafficChart');
                                        if (!chartContainer) return;
                                        
                                        const observer = new IntersectionObserver((entries) => {
                                            entries.forEach(entry => {
                                                if (entry.isIntersecting) {
                                                    initChart();
                                                    observer.unobserve(entry.target);
                                                }
                                            });
                                        }, { 
                                            rootMargin: '100px' // Load when chart is 100px away from viewport
                                        });
                                        
                                        observer.observe(chartContainer);
                                    }
                                    
                                    // Initialize when DOM is ready
                                    if (document.readyState === 'loading') {
                                        document.addEventListener('DOMContentLoaded', observeChart);
                                    } else {
                                        observeChart();
                                    }
