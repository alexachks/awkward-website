    <!-- AI Domination Block Interactive Script -->
    <script>
        document.addEventListener('DOMContentLoaded', function() {
            const aiBlock = document.querySelector('.ai-domination-block');
            const brainContainer = document.querySelector('.ai-brain-container');
            const supportingElements = document.querySelectorAll('.supporting-element');
            
            if (!aiBlock || !brainContainer) return;

            // Enhanced hover interactions for supporting elements
            supportingElements.forEach((element, index) => {
                element.addEventListener('mouseenter', function() {
                    // Brighten the connecting line
                    const line = element.querySelector('.supporting-line');
                    if (line) {
                        line.style.opacity = '1';
                        line.style.strokeWidth = '2';
                        line.style.filter = 'drop-shadow(0 0 3px currentColor)';
                    }
                    
                    // Add tooltip effect
                    const tooltip = element.querySelector('.bg-neutral-900\\/90');
                    if (tooltip) {
                        tooltip.style.transform = 'scale(1.05)';
                        tooltip.style.borderColor = 'rgba(168, 85, 247, 0.6)';
                    }
                });
                
                element.addEventListener('mouseleave', function() {
                    const line = element.querySelector('.supporting-line');
                    if (line) {
                        line.style.opacity = '';
                        line.style.strokeWidth = '';
                        line.style.filter = '';
                    }
                    
                    const tooltip = element.querySelector('.bg-neutral-900\\/90');
                    if (tooltip) {
                        tooltip.style.transform = '';
                        tooltip.style.borderColor = '';
                    }
                });
            });

            // Add Intersection Observer for performance
            const observer = new IntersectionObserver((entries) => {
                entries.forEach(entry => {
                    if (entry.isIntersecting) {
                        // Start animations when visible
                        aiBlock.classList.add('ai-visible');
                    } else {
                        // Pause some animations when not visible for performance
                        aiBlock.classList.remove('ai-visible');
                    }
                });
            }, {
                threshold: 0.3,
                rootMargin: '50px'
            });

            observer.observe(aiBlock);

            // Add click interaction to neural sphere for demo purposes
            const neuralCore = document.querySelector('.neural-core');
            if (neuralCore) {
                neuralCore.addEventListener('click', function() {
                    // Create ripple effect
                    const ripple = document.createElement('div');
                    ripple.style.cssText = `
                        position: absolute;
                        top: 50%;
                        left: 50%;
                        width: 4px;
                        height: 4px;
                        border-radius: 50%;
                        background: rgba(168, 85, 247, 0.6);
                        transform: translate(-50%, -50%);
                        animation: ripple 1s ease-out forwards;
                        pointer-events: none;
                        z-index: 10;
                    `;
                    
                    // Add ripple keyframes if not already added
                    if (!document.querySelector('#ripple-animation')) {
                        const style = document.createElement('style');
                        style.id = 'ripple-animation';
                        style.textContent = `
                            @keyframes ripple {
                                to {
                                    width: 100px;
                                    height: 100px;
                                    opacity: 0;
                                }
                            }
                        `;
                        document.head.appendChild(style);
                    }
                    
                    neuralCore.appendChild(ripple);
                    
                    // Remove ripple after animation
                    setTimeout(() => {
                        if (ripple.parentNode) {
                            ripple.parentNode.removeChild(ripple);
                        }
                    }, 1000);
                });
            }

            // Parallax effect removed - animation stays in place
            
            // Testimonials Carousel
            const testimonialCarousel = document.querySelector('.testimonial-carousel');
            if (testimonialCarousel) {
                const cards = testimonialCarousel.querySelectorAll('.testimonial-card');
                const dots = testimonialCarousel.querySelectorAll('.testimonial-dot');
                let currentIndex = 0;
                let intervalId;

                function showTestimonial(index) {
                    // Ensure valid index
                    if (index < 0 || index >= cards.length) return;
                    
                    // Stop any existing auto-rotation during manual transition
                    clearInterval(intervalId);
                    
                    // Remove active class from all cards and dots
                    cards.forEach((card, i) => {
                        card.classList.remove('testimonial-active');
                        card.style.opacity = '0';
                    });
                    
                    dots.forEach((dot, i) => {
                        dot.classList.remove('dot-active');
                        dot.style.backgroundColor = 'rgb(75 85 99)'; // gray-600
                        dot.style.boxShadow = 'none';
                    });

                    // Show current card with small delay for smooth transition
                    setTimeout(() => {
                        const currentCard = cards[index];
                        if (currentCard) {
                            currentCard.classList.add('testimonial-active');
                            currentCard.style.opacity = '1';
                        }
                        
                        // Update active dot
                        const currentDot = dots[index];
                        if (currentDot) {
                            currentDot.classList.add('dot-active');
                            currentDot.style.backgroundColor = 'rgb(168 85 247)';
                            currentDot.style.boxShadow = '0 0 8px rgba(168, 85, 247, 0.6)';
                        }
                    }, 100);
                }

                function nextTestimonial() {
                    currentIndex = (currentIndex + 1) % cards.length;
                    showTestimonial(currentIndex);
                }

                function startAutoRotation() {
                    stopAutoRotation(); // Clear any existing interval
                    intervalId = setInterval(() => {
                        nextTestimonial();
                    }, 10000); // 10 seconds
                }

                function stopAutoRotation() {
                    if (intervalId) {
                        clearInterval(intervalId);
                        intervalId = null;
                    }
                }

                // Click handlers for dots
                dots.forEach((dot, index) => {
                    dot.addEventListener('click', () => {
                        if (currentIndex !== index) { // Only change if different
                            currentIndex = index;
                            showTestimonial(currentIndex);
                            // Restart auto-rotation after user interaction
                            setTimeout(() => {
                                startAutoRotation();
                            }, 3000); // Resume after 3 seconds
                        }
                    });
                });

                // Pause on hover, resume on leave
                testimonialCarousel.addEventListener('mouseenter', stopAutoRotation);
                testimonialCarousel.addEventListener('mouseleave', startAutoRotation);

                // Initialize
                showTestimonial(0);
                startAutoRotation();
            }
        });
    </script>

